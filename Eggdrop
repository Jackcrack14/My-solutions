# Solution to Egg drop problem
def eggdrop(n,k):
    max_chance = 2 * n
    return min(max_chance,min_prob(k))

def min_prob(k) :
    arr = [0 for i in range(k)]

    arr[0] = 1
    arr[1] = 2
    i = 1
    while (arr[i]<= k) :
        i = i+1
        arr[i] = arr[i-1] + arr[i-2]

    return i - 1

min_prob(10)
print(eggdrop(4,20))
